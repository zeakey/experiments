read_write: read_write.cpp
	g++ read_write.cpp  -g -L/usr/local/lib -lopencv_core -lopencv_imgcodecs \
	-lopencv_core -lopencv_highgui -lopencv_imgproc -lopencv_imgcodecs -o read_write.o


io: io.cpp
	g++ io.cpp  -g -L/usr/local/lib -lopencv_core -lopencv_imgcodecs \
	-lopencv_core -lopencv_highgui -lopencv_imgproc -lopencv_imgcodecs -o io.o

clean:
	rm *.o
